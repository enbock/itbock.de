{
  "name": "ai-webpage-itbock.de-2024",
  "version": "1.0.0",
  "dependencies": {
    "@aws-sdk/client-polly": "*",
    "@enbock/ts-jsx": "*",
    "@types/jsdom": "*",
    "@types/node": "*",
    "copy-webpack-plugin": "*",
    "css-loader": "*",
    "html-webpack-plugin": "*",
    "jsdom": "*",
    "ts-loader": "*",
    "typescript": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "scripts": {
    "dev:ai": "npx ai-code-writer",
    "start": "webpack serve --open --mode development --no-open --no-live-reload --no-hot --stats-error-details",
    "build": "webpack --mode production",
    "deploy": "npm-run-all --serial build deploy:clean deploy:copy",
    "deploy:clean": "aws s3 rm s3://www.itbock.de/ --recursive --exclude \"*\" --include \"bundle.*\"",
    "deploy:copy": "aws s3 sync build s3://www.itbock.de/"
  },
  "devDependencies": {
    "dotenv": "*",
    "dotenv-webpack": "*",
    "npm-run-all": "^4.1.5"
  }
}
